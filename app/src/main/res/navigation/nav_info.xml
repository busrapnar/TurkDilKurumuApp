<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_info"
    app:startDestination="@id/menuFragment">

    <fragment
        android:id="@+id/menuFragment"
        android:name="com.busrapinar.turkdilkurumuapp.view.fragment.MenuFragment"
        android:label="MenuFragment" >
        <action
            android:id="@+id/action_menuFragment_to_contactFragment2"
            app:destination="@id/contactFragment2" />
        <action
            android:id="@+id/action_menuFragment_to_aboutusFragment2"
            app:destination="@id/aboutusFragment2" />
    </fragment>
    <fragment
        android:id="@+id/contactFragment2"
        android:name="com.busrapinar.turkdilkurumuapp.view.fragment.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/aboutusFragment2"
        android:name="com.busrapinar.turkdilkurumuapp.view.fragment.AboutusFragment"
        android:label="fragment_aboutus"
        tools:layout="@layout/fragment_aboutus" />
</navigation>